{
  "name": "react-rotation",
  "description": "React rotation component",
  "version": "0.0.0",
  "repository": "andrepolischuk/react-rotation",
  "dependencies": {
    "bootstrap-gh-pages": "^0.2.1",
    "events": "^1.0.2",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-rotation": "^0.3.0"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "postcss": "^4.1.11",
    "postcss-cli": "^1.3.1",
    "postcss-import": "^5.2.2"
  },
  "scripts": {
    "prebuild": "rm -rf build && mkdir build",
    "build": "npm run build-js && npm run build-css",
    "build-js": "browserify -t [babelify --stage 0] index.js > build/index.js",
    "build-css": "postcss --use postcss-import index.css > build/index.css"
  },
  "author": "Andrey Polischuk <andre.polischuk@gmail.com>",
  "license": "MIT"
}
